import java.util.Scanner;

public class Exercicios {
	
	public static void main(String[] args) {
		
		 Scanner leia = new Scanner(System.in);
		int A;
		 int B;
		 int X;
		 
	
		 A = leia.nextInt();
		
		 B = leia.nextInt();
		 
		 X = A + B;
		        System.out.println("X = "+ X);
		        System.out.println("Fim de linha");
		
		
		
		
		
	}
	System.out.println("Fim de linha");
}
